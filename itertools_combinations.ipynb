      "source": [
        "from itertools import combinations\n",
        "\n",
        "s, k = input().split(\" \")\n",
        "for i in range(1 ,int(k)+1):\n",
        "    for j in combinations(sorted(s),i):\n",
        "        print(\"\".join(j))\n"
      ]
    }
