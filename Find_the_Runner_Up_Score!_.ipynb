      "source": [
        "if __name__ == '__main__':\n",
        "    n = int(raw_input())\n",
        "    arr = map(int, raw_input().split())\n",
        "    zes = max(arr)\n",
        "    i=0\n",
        "    while(i<n):\n",
        "        if zes ==max(arr):\n",
        "            arr.remove(max(arr))\n",
        "        i+=1\n",
        "    print(max(arr))\n",
